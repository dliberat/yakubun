{
  "name": "yakubun",
  "version": "0.2.5",
  "description": "Library for checking Japanese translations",
  "main": "dist/yakubun.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/garroadran/yakubun"
  },
  "author": "Dan Liberatori <daniel.liberatori@gmail.com>",
  "dependencies": {
    "comma-number": "^2.0.0",
    "moment-timezone": "^0.5.13",
    "ordinal": "^1.0.2",
    "yakubun-utils": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "docdash": "^0.4.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4"
  },
  "engines": {
    "node": ">=9.6.1"
  },
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
